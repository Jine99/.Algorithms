{
  "cmake.configureOnOpen": true,
  "cmake.buildDirectory": "${workspaceFolder}/build",
  "cmake.parallelJobs": 4,
  "cmake.configureSettings": {
    "CMAKE_BUILD_TYPE": "Debug"
  },
  "cmake.generator": "MinGW Makefiles",
  "cmake.debugConfig": {
    "cwd": "${workspaceFolder}"
  },
  "files.associations": {
    "*.cpp": "cpp",
    "*.h": "cpp",
